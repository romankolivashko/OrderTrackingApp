@{
  Layout = "_Layout";
}

@using VendorTracker.Models;

<h3>All Vendors:</h3>

@if (@Model.Count == 0)
{
  <h4>No vendors have been added yet!</h4>
} 

@foreach (Vendor vendor in Model)
{
  <h4><li><a href='/vendors/@vendor.Id'>@vendor.Name</a></li></h4>
}

<p><button><a href='/'>Back home</a></button></p>